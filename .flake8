[flake8]
max-line-length = 88
extend-ignore = 
    E203,
    W503,
    E501,
    F401,
    F403,
    F405,
    W291,
    W292,
    W293
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    model_cache,
    outputs,
    temp,
    uploads
per-file-ignores =
    __init__.py:F401,F403
    tests/*:F401,F403 